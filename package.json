{
  "name": "estun-robot-highlighting",
  "displayName": "埃斯顿（ESTUN）机器人RCS2语法高亮",
  "description": "语法高亮显示ESTUN机器人RCS2编程语言(.erd和.erp文件)",
  "version": "1.1.2",
  "publisher": "AhWei",
  "engines": {
    "vscode": "^1.0.0"
  },
  "categories": [
    "Languages"
  ],
  "keywords": [
    "estun",
    "rcs2",
    "robot",
    "programming",
    "syntax highlighting"
  ],
  "homepage": "https://github.com/C-Li/ESTUN_Robot_Highlighting",
  "repository": {
    "type": "git",
    "url": "https://github.com/C-Li/ESTUN_Robot_Highlighting.git"
  },
  "bugs": {
    "url": "https://github.com/C-Li/ESTUN_Robot_Highlighting/issues"
  },
  "main": "./out/extension.js",
  "activationEvents": [
    "onLanguage:robot-language"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "contributes": {
    "languages": [
      {
        "id": "robot-language",
        "aliases": [
          "ESTUN RCS2 Robot Language",
          "estun-rcs2-robot-language",
          "RCS2 Robot Language"
        ],
        "extensions": [
          ".erd",
          ".erp"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "robot-language",
        "scopeName": "source.robot-language",
        "path": "./syntaxes/robot-language.tmLanguage.json"
      }
    ]
  },
  "devDependencies": {
    "@vscode/vsce": "^3.7.1",
    "@types/node": "^20.0.0",
    "@types/vscode": "^1.0.0",
    "typescript": "^5.0.0"
  }
}
